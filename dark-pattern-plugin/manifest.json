{
  "manifest_version": 2,
  "name": "deceptive-insight",
  "version": "1.0",
  "description": "A plugin to identify dark patterns in user reviews",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "cat.png",
    "default_popup": "popup.html",
    "default_title": "Click me!"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}